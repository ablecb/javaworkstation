package stringtest;
public class stringTest {
	public static void main(String[] args) {
		String helloWorld ="hello world";
		String hello="hello";
		//System.out.println(helloWorld=="hello"+" world");
		System.out.println(helloWorld==hello+" world");
		System.out.println(helloWorld.intern()==("hello"+" world").intern());
	}
}
